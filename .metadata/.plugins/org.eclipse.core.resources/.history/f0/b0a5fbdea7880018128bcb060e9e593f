package com.gxnnbsd.charger.wx.entity.menu;

import com.gxnnbsd.modules.business.wx.entity.TWxMenu;

public class ScancodeWaitmsg extends BaseMenu {
	private String type = "scancode_waitmsg";
	private String key;
	
	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getKey() {
		return key;
	}

	public void setKey(String key) {
		this.key = key;
	}
	
	public ScancodeWaitmsg(){
	}
	public ScancodeWaitmsg(TWxMenu wxMenu){
		super(wxMenu);
		this.key = wxMenu.getId().toString();
	}
}
